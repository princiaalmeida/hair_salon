{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "es2017",
    "lib": ["ES2017"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["vite.config.ts"]
}
